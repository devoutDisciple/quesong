.address{
	height: 100vh;
	width: calc(100vw - 20px);
	background: #fff;
	overflow: hidden;
	padding: 10px;
	font-size: 14px;
	color: #333;
}
.address_row{
	border-bottom: 1px solid #ebebeb;
	padding: 15px 0;
}
.address_row_label{
	margin-right: 10px;
}
.address_row_label_special{
	color: #fff;
}
.address_row_radio_col_second{
	margin-left: 30px;
}
.address_row_btn{
	margin-top: 30px;
}
.address_row_btn button{
	background: #6af5d0;
	color: #fff;
}
